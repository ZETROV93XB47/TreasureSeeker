CREATE TABLE AWARE_USER_ROLE (
    userIgg BIGINT AUTO_INCREMENT PRIMARY KEY,
    username VARCHAR(256) UNIQUE NOT NULL,
    profile VARCHAR(256),
    first_name VARCHAR(256),
    last_name VARCHAR(256),
    credit_catalog VARCHAR(10),
    rate_catalog VARCHAR(10),
    wwr_dr_catalog VARCHAR(10),
    equity_catalog VARCHAR(10),
    dfa_catalog VARCHAR(10),
    im_schedule_catalog VARCHAR(10),
    simmp_catalog VARCHAR(10),
    stt_catalog VARCHAR(10),
    initial_date TIMESTAMP,
    modification_date TIMESTAMP,
    windows_login VARCHAR(14),
    admin VARCHAR(10),
    c2_insider_catalog VARCHAR(10),
    varsvar_catalog VARCHAR(10)
);